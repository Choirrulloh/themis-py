#!/bin/sh
# postinst script for themis

set -e

fix_perms() {
    chmod 755 /usr/sbin/tmscli
}

case "$1" in
    configure)
        fix_perms
    ;;

    abort-upgrade|abort-remove|abort-deconfigure)
    ;;

    *)
        echo "postinst called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

#DEBHELPER#

exit 0

